<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d"
             d:DesignWidth="800" d:DesignHeight="450"
             xmlns:c="clr-namespace:Yuyuyui.PrivateServer.GUI.Controls"
             x:Class="Yuyuyui.PrivateServer.GUI.Controls.ConsolePage">

  <Panel HorizontalAlignment="Stretch" VerticalAlignment="Stretch">

    <StackPanel Orientation="Vertical" Spacing="10">

      <c:LogEntryControl LogContent="Test Log Trace" LogLevel="Trace"></c:LogEntryControl>
      <c:LogEntryControl LogContent="Test Log Log" LogLevel="Log"></c:LogEntryControl>
      <c:LogEntryControl LogContent="Test Log Warning" LogLevel="Warning"></c:LogEntryControl>
      <c:LogEntryControl LogContent="Test Log Error" LogLevel="Error"></c:LogEntryControl>

    </StackPanel>

  </Panel>

</UserControl>